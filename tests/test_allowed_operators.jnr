# Test allowed operators after removal of bitwise and logical operators

# Arithmetic operators
int a = 10 + 5!
show(a)!

int b = 20 - 8!
show(b)!

int c = 6 * 7!
show(c)!

int d = 100 / 4!
show(d)!

int e = 17 % 5!
show(e)!

# Comparison operators
int f = 5 < 10!
show(f)!

int g = 10 > 5!
show(g)!

int h = 5 <= 5!
show(h)!

int i = 10 >= 10!
show(i)!

int j = 5 == 5!
show(j)!

int k = 5 != 3!
show(k)!

# Complex expressions with parentheses
int l = (10 + 5) * 2!
show(l)!

int m = 2 + 3 * 4!
show(m)!

# Real numbers
real pi = 3.14159!
show(pi)!

real area = pi * 5.0 * 5.0!
show(area)!
