# Test operators - Updated to remove bitwise and logical operators
# Only arithmetic, comparison, and modulo operators are now supported

int a = 10 + 5 * 2;
print(a);

int b = 17 % 5;
print(b);

# Bitwise operators removed - these would cause syntax errors:
# int c = 12 & 10;
# int d = 12 | 10;
# int e = 12 ^ 10;
# int f = 5 << 2;
# int g = 20 >> 2;

int h = 5 < 10;
print(h);

int i = 5 > 10;
print(i);

int j = 5 == 5;
print(j);

int k = 5 != 3;
print(k);

# Logical operators removed - these would cause syntax errors:
# int l = 1 && 1;
# int m = 1 || 0;
# int n = 2 + 3 * 4 & 15;
# int o = (5 > 3) && (10 < 20);

int p = 3 + 4 * 2;
print(p);
